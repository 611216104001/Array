digit = int(input())
str_dig = [int(x) for x in str(digit)]
no_odd = []
odd = []
for i in range(0,len(str_dig)):
    if ((str_dig[i] % 2) != 0):
        odd.append(str_dig[i])
for i in range(0,len(str_dig)):
    if ((str_dig[i] % 2) == 0):
        no_odd.append(-1)
if(len(odd) == 0):
    print(no_odd[0])
else:
    print(*odd)
    
    
    
